{
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"bracketSameLine": true,
	"htmlWhitespaceSensitivity": "ignore",
	"insertPragma": false,
	"jsxBracketSameLine": false,
	"jsxSingleQuote": true,
	"printWidth": 80,
	"proseWrap": "always",
	"quoteProps": "as-needed",
	"requirePragma": false,
	"singleQuote": true,
	"tabWidth": 2,
	"trailingComma": "es5",
	"singleAttributePerLine": true,
	"useTabs": false,
	"semi": true
}
